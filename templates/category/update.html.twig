{% extends "base.html.twig" %}

{% block body %}
	<div class="container">
		<h2>Edit Category</h2>
		{{form_start (form)}}
		<div>
			<div class="form-group">
				{{form_label (form.name)}}
				{{form_widget (form.name,{'attr::class':'form.control'})}}
			</div>
			<div class="form-group mt-3">
				<button class="btn btn-primary">Update</button>
			</div>
		</div>
		{{form_end (form)}}
	</div>
{% endblock %}
